<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              Title="Form M"
                          xmlns:custom="clr-namespace:RAMMS.MobileApps.Controls"

             xmlns:forms="clr-namespace:SignaturePad.Forms;assembly=SignaturePad.Forms"
             xmlns:page="clr-namespace:RAMMS.MobileApps.Page"
             xmlns:local="clr-namespace:RAMMS.MobileApps"
             xmlns:locals="clr-namespace:RAMMS.MobileApps.Controls;assembly=RAMMS.MobileApps"
             x:Class="RAMMS.MobileApps.Page.FormMAddPage">
    <ContentPage.Resources>
        <ResourceDictionary>
            <local:NegateBoolConverter x:Key="NegatebooleanConverter" />
            <page:C1C2ImageSrcConverter x:Key="C1C2ImageSrcConverter"/>
        </ResourceDictionary>
    </ContentPage.Resources>
    <ContentPage.Content>
        <StackLayout>

            <Frame Margin="0,0,0,0" HasShadow="True" BorderColor="#e6e6e6">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="45"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>

                    <Image Source="JKRSLogo.png" Grid.Row="0" Grid.Column="0" Margin="0,-10,0,0"/>
                    <Label Text="ROAD ASSET MANAGEMENT AND MAINTENANCE SYSTEM" LineBreakMode="TailTruncation" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" TextColor="Black" FontAttributes="Bold" FontSize="Large" Grid.Row="0" Grid.Column="2"  Margin="0,-10,0,0"/>
                    <Image Source="ECSBLogo.png" Grid.Row="0" Grid.Column="4"  Margin="0,-10,0,0"/>


                </Grid>
            </Frame>
            <StackLayout>
                <Frame>
                    <StackLayout>
                        <Grid>
                            <Grid.RowDefinitions>

                                <RowDefinition Height="Auto"/>

                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2"/>
                                <ColumnDefinition Width="4*"/>
                                <ColumnDefinition Width="2"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Orientation="Horizontal" Grid.Row="0" Grid.Column="1" HorizontalOptions="Center">
                                <Label   Text="Form M No" FontSize="Large"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="3"  ColorName="LightGray"    BorderColor="Black" >
                                    <Label   Text="{Binding SelectedRefNo}" FontSize="Large"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                </locals:FrameRenders>
                            </StackLayout>
                        </Grid>
                    </StackLayout>
                </Frame>
            </StackLayout>

            <ScrollView>
                <StackLayout >

                    <Frame  BackgroundColor="White" BorderColor="#bfbfbf" CornerRadius="5"  Margin="5,0,5,0"  >
                        <StackLayout>
                            <Grid RowSpacing="15" ColumnSpacing="9" IsVisible="{Binding FromAdd}"  >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>


                                <Label Text="Division" Grid.Row="0" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                <locals:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDDivisionListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedDivision, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" CharacterSpacing="20"  Title="Select Division"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>





                                <Label Text="RMU" Grid.Row="0" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="2" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDRMUListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedRMU, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select RMU"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>


                                <Label Text="Section Code"  Grid.Row="0" Grid.Column="3"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="3" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDSectionListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedSectionCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Section Code"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>


                                <Label Text="Section Name" Grid.Row="0" Grid.Column="4"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                <locals:FrameRenders Grid.Row="1"  Grid.Column="4" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl  IsEnabled="{Binding IsHeaderEnable}" Text="{Binding SelectedSectionName}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" />
                                </locals:FrameRenders>

                                <Label Text="Road Code" Grid.Row="2" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                <locals:FrameRenders Grid.Row="3"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker  ItemsSource="{Binding DDRodeCodeListItems}"  ItemDisplayBinding="{Binding Text}"  SelectedItem="{Binding SelectedRoadCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Road Code"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"    TextColor="Black" />
                                </locals:FrameRenders>

                                <Label Text="Road Name" Grid.Row="2" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="3"  Grid.Column="2" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl IsEnabled="{Binding IsHeaderEnable}" Text="{Binding SelectedRoadName}"  TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  InputTransparent="True" />
                                </locals:FrameRenders>


                                <Label Text="Activity Code"  Grid.Row="2" Grid.Column="3"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="3"  Grid.Column="3" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker  ItemsSource="{Binding DDActivityListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedActivity, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}"  Title="Select Activity Code"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>


                                <Label Text="Activity Name" Grid.Row="2" Grid.Column="4"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                <locals:FrameRenders Grid.Row="3"  Grid.Column="4" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl  IsEnabled="{Binding IsHeaderEnable}" Text="{Binding SelectedActivityName}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" />
                                </locals:FrameRenders>
                                
                                <Label Text="Location CH" Grid.Row="4" Grid.Column="1"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <StackLayout HeightRequest="30" HorizontalOptions="FillAndExpand" Orientation="Horizontal" Grid.Row="5"  Grid.Column="1" >

                                    <locals:FrameRenders HorizontalOptions="FillAndExpand" Margin="0,0,3,0" HeightRequest="37" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                        <custom:CustomEntryNumeric IsEnabled="{Binding IsHeaderEnable}" Margin="0,0,0,-7" HorizontalOptions="FillAndExpand" MaxLength="5" Text="{Binding LocChKm}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Numeric"  />
                                    </locals:FrameRenders>
                                    <Label  Text="+"  FontSize="Large" FontAttributes="Bold"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" VerticalTextAlignment="Center" />

                                    <locals:FrameRenders HorizontalOptions="FillAndExpand" Margin="3,0,0,0" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                        <custom:CustomEntryNumeric IsEnabled="{Binding IsHeaderEnable}" Margin="0,0,0,-7" HorizontalOptions="FillAndExpand" MaxLength="3"  Text="{Binding LocChM}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Numeric"  />
                                    </locals:FrameRenders>

                                </StackLayout>

                                <Label Text="Audit Date" FontSize="Medium" TextColor="Black" Opacity="0.8" Grid.Row="4" Grid.Column="2"    FontFamily="{StaticResource ProximaNovaRegularFont}"  />

                                <locals:FrameRenders Grid.Row="5"  Grid.Column="2" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                    <local:CustomDatePicker NullText="dd/mm/yyyy" IsEnabled="{Binding IsHeaderEnable}" NullableDate="{Binding AuditedDate}" InputTransparent="{Binding NotToEdit}" Format="dd-MM-yyyy" BackgroundColor="White"   VerticalOptions="CenterAndExpand" FontSize="16"  HorizontalOptions="FillAndExpand" TextColor="Gray"   />
                                </locals:FrameRenders>



                                <Label Text="Audit Time From" FontSize="Medium" TextColor="Black" Opacity="0.8" Grid.Row="4" Grid.Column="3"    FontFamily="{StaticResource ProximaNovaRegularFont}"  />

                                <locals:FrameRenders Grid.Row="5"  Grid.Column="3" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <TimePicker x:Name="fAuditftime" IsEnabled="{Binding IsHeaderEnable}" Time="{Binding SavefTime, StringFormat='{}{0:hh\\:mm}', Mode=TwoWay}" TextColor="Gray"/>
                                </locals:FrameRenders>

                                <Label Text="Audit Time To" FontSize="Medium" TextColor="Black" Opacity="0.8" Grid.Row="4" Grid.Column="4"    FontFamily="{StaticResource ProximaNovaRegularFont}"  />

                                <locals:FrameRenders Grid.Row="5"  Grid.Column="4" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <TimePicker  x:Name="tAuditttime" IsEnabled="{Binding IsHeaderEnable}" Format="hh:mm tt" Time="{Binding SavetTime, Mode=TwoWay}"  TextColor="Gray"/>

                                </locals:FrameRenders>
                                <Button Grid.Row="6"  IsEnabled="{Binding IsHeaderEnable}"  Grid.Column="4" HorizontalOptions="End" VerticalOptions="End" TextColor="White" Command="{Binding OKCommand}" CornerRadius="10" FontSize="17" BackgroundColor="#1D2D50" Text="Find Details"></Button>
                            
                            
                            </Grid>
                          
                            
                            
                            <!--<Grid  IsVisible="{Binding FromAdd, Converter={StaticResource NegatebooleanConverter}}" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />

                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />

                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                --><!--<Label Text="Asset ID"  Grid.Row="0" Grid.Column="0"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="0" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl HeightRequest="40" Text="{Binding AiAssetId}"  IsEnabled="{Binding IsHeaderEnable}"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>

                                <Label Text="Year of Inspection (YYYY)" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2" FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"  BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDYearListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedYear, Mode=TwoWay}"   IsEnabled="{Binding IsHeaderEnable}" ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>--><!--

                            </Grid>-->
                        </StackLayout>
                    </Frame>


                    <Frame  BackgroundColor="White" BorderColor="#bfbfbf" CornerRadius="5"  Margin="5,0,5,0"  >
                        <StackLayout>
                            <Grid RowSpacing="15" ColumnSpacing="9"  >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />

                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>


                                <Label Text="Reference No" Grid.Row="0" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl  Text="{Binding RefId}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" />
                                </locals:FrameRenders>




                                <Label Text="Type" Grid.Row="0" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="2" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker SelectedItem="{Binding SelectedType, Mode=TwoWay}" IsEnabled="{Binding CanSave}" Title="Select Type"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black">
                                    <local:ExtendedPicker.Items>
                                            <x:String>Partial</x:String>
                                            <x:String>Full Road Closure</x:String>
                                    </local:ExtendedPicker.Items>
                                    </local:ExtendedPicker>

                                </locals:FrameRenders>


                                <Label Text="Method"  Grid.Row="0" Grid.Column="3"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="3" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker SelectedItem="{Binding SelectedMethod, Mode=TwoWay}" IsEnabled="{Binding CanSave}" Title="Select Method"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" >
                                        <local:ExtendedPicker.Items>
                                            <x:String>Stop-Go</x:String>
                                            <x:String>Diversion</x:String>
                                            <x:String>Others</x:String>
                                        </local:ExtendedPicker.Items>
                                    </local:ExtendedPicker>
                                </locals:FrameRenders>


                                <Label Text="Crew Supervisor" Grid.Row="0" Grid.Column="4"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                
                                <locals:FrameRenders Grid.Row="1"  Grid.Column="4" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDCrewUserListListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedCrewIndex, Mode=TwoWay}"  IsEnabled="{Binding CanSave}" CharacterSpacing="20"  Title="Select Users"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>
                                <Label Text="Audit Plan" Grid.Row="2" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                <locals:FrameRenders Grid.Row="3"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker  SelectedItem="{Binding SelectedAuditplan, Mode=TwoWay}" IsEnabled="{Binding CanSave}" Title="Select"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"    TextColor="Black">
                                    <local:ExtendedPicker.Items>
                                            <x:String>Plan</x:String>
                                            <x:String>Unplanned</x:String>
                                   </local:ExtendedPicker.Items>
                                    </local:ExtendedPicker>
                                </locals:FrameRenders>

                                <Label Text="Audited by" Grid.Row="2" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                               

                                <locals:FrameRenders Grid.Row="3"  Grid.Column="2" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker SelectedIndex="{Binding SelectedAuditBy, Mode=TwoWay}"  IsEnabled="{Binding CanSave}"  Title="Select Activity Code"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black">
                                    <local:ExtendedPicker.Items>
                                            <x:String>SJP</x:String>
                                        <x:String>E.C</x:String>
                                        <x:String>S.O</x:String>
                                    </local:ExtendedPicker.Items>
                                    </local:ExtendedPicker>
                                </locals:FrameRenders>
                                
                                <Label Text="Name of Service(S.P)"  Grid.Row="2" Grid.Column="3"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                              
                                <locals:FrameRenders Grid.Row="3"  Grid.Column="3" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl IsEnabled="{Binding IsHeaderEnable}" Text="Endaya Construction"  TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  InputTransparent="True" />
                                </locals:FrameRenders>

                               

                            </Grid>



                        </StackLayout>
                    </Frame>



                    <StackLayout x:Name="stack1" Margin="0,-10,0,0" IsVisible="{Binding IsPhotoTabVisible, Converter={StaticResource NegatebooleanConverter}}">
                        <StackLayout>
                            <Frame BorderColor="Gray" CornerRadius="12" BackgroundColor="White" HasShadow="True" Margin="10">
                                <StackLayout>

                                    <StackLayout >
                                        <Frame BackgroundColor="#f2f2f2" HasShadow="True" BorderColor="#e6e6e6">
                                            <StackLayout Orientation="Horizontal">
                                                <Image x:Name="formToggleImage" Source="RoundedAddIcon.png" HeightRequest="35" WidthRequest="35" VerticalOptions="Start" HorizontalOptions="Start">
                                                </Image>
                                                <Label Text="Signs" Margin="10,0,0,0" FontSize="Large" TextColor="Black" HorizontalOptions="StartAndExpand" VerticalOptions="StartAndExpand">

                                                </Label>
                                            </StackLayout>
                                            <Frame.GestureRecognizers>
                                                <TapGestureRecognizer Command="{Binding ToggleCommand}" CommandParameter="{x:Reference FormDLayout}"/>
                                                <TapGestureRecognizer Command="{Binding ToggleCommand}" CommandParameter="{x:Reference formToggleImage}"/>
                                            </Frame.GestureRecognizers>
                                        </Frame>

                                        <Frame x:Name="FormDLayout"  Margin="0,0,0,0" BackgroundColor="White"   HasShadow="True" BorderColor="#bfbfbf"  IsVisible="False" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                                            <StackLayout>
                                                <Grid RowSpacing="15"  ColumnSpacing="15" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="25" />
                                                        <RowDefinition Height="1" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />
                                                        <RowDefinition Height="45" />

                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="50" />
                                                        <ColumnDefinition Width="120" />
                                                        <ColumnDefinition Width="70" />
                                                        <ColumnDefinition Width="30" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />
                                                    </Grid.ColumnDefinitions>

                                                    <BoxView Grid.Row="1" BackgroundColor="Gray" Grid.Column="0"   Grid.ColumnSpan="6"/>

                                                    <Label Text="A1" Grid.Row="2" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A2" Grid.Row="3" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A3" Grid.Row="4" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A4" Grid.Row="5" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A5" Grid.Row="6" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A6" Grid.Row="7" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A7" Grid.Row="8" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="A8" Grid.Row="9" Grid.Column="0"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    
                                                    <Label Text="Sign" Grid.Row="0" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="Missing" Grid.Row="2" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="Position" Grid.Row="3" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="Not Visible/Fallen Over" Grid.Row="4" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
                                                    <Label Text="Wrong Sign" Grid.Row="5" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />
